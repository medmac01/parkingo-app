{
  "expo": {
    "name": "uemf-park",
    "slug": "uemf-park",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "splash": {
        "ldpi": "./app/assets/splash/android/drawable-ldpi/splash.png",
        "mdpi": "./app/assets/splash/android/drawable-mdpi/splash.png",
        "hdpi": "./app/assets/splash/android/drawable-hdpi/splash.png",
        "xhdpi": "./app/assets/splash/android/drawable-xhdpi/splash.png",
        "xxhdpi": "./app/assets/splash/android/drawable-xxhdpi/splash.png",
        "xxxhdpi": "./app/assets/splash/android/drawable-xxxhdpi/splash.png"
      },
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "",
    "githubUrl": "https://github.com/medmac01/uemf-park"
  }
}
